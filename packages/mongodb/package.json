{
  "name": "@awesome-backup/mongodb",
  "version": "0.0.1",
  "description": "Collection of scripts which backup MongoDB databases to Amazon S3 or Google Cloud Storage. You can set a custom S3 endpoint to use S3 based services like DigitalOcean Spaces instead of Amazon S3.",
  "author": "Ryu Sato <ryu@weseek.co.jp>",
  "license": "MIT",
  "homepage": "https://github.com/ryu-sato/awesome-backup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-sato/awesome-backup.git"
  },
  "bugs": {
    "url": "https://github.com/ryu-sato/awesome-backup/issues"
  },
  "main": "dist/mongodb/src/index.js",
  "types": "dist/mongodb/src/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "// for development": "",
    "ts-node-dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@awesome-backup/core": "^0.0.1",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0"
  },
  "devDependencies": {
    "tsconfig-paths": "^3.12.0"
  },
  "engines": {
    "node": ">= 16.13.1 < 17",
    "yarn": ">= 1.22.4"
  }
}
